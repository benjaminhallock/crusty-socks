# Production environment variables
NODE_ENV=production
MONGO_URI=mongodb+srv://<username>:<password>@<cluster>.mongodb.net/crustysocks
PORT=8081

# Client configuration
# This will be the CloudFront distribution URL
CLIENT_URL=https://<distribution-id>.cloudfront.net

# AWS configuration
AWS_REGION=us-east-1
S3_BUCKET=crustysocks-client

# Token settings
JWT_SECRET=your-production-jwt-secret
JWT_EXPIRE=30d

# Rate limiting
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX=100
